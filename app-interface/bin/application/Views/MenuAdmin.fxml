							<font>
								<Font name="Segoe UI" size="18.0" />
							</font>
							<HBox.margin>
								<Insets right="5.0" />
							</HBox.margin>
						</Label>
						<ImageView fx:id="imgLogout" fitHeight="44.0"
							fitWidth="44.0" onMouseClicked="#clickLogout" pickOnBounds="true"
							preserveRatio="true">
							<image>
								<Image url="@../../Logout.png" />
							</image>
							<HBox.margin>
								<Insets right="10.0" />
							</HBox.margin>
						</ImageView>
					</children>
