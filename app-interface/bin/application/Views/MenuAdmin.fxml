								<Insets top="10.0" />
							</VBox.margin>
						</ImageView>
						<Label text="Fornecedores">
							<font>
								<Font name="Segoe UI" size="15.0" />
							</font>
						</Label>
					</children>
				</VBox>
				<VBox alignment="TOP_CENTER" prefHeight="336.0"
					prefWidth="115.0">
					<children>
						<ImageView fx:id="imgCashier" fitHeight="70.0"
							fitWidth="70.0" onMouseClicked="#clickCashier"
							pickOnBounds="true" preserveRatio="true">
